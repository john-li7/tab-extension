{
  "manifest_version": 2,
  "name": "tab manANGER",
  "version": "1.0",
  "description": "Chrome tab organizer",
  "permissions": [
    "<all_urls>",
    "tabs"
  ],
  "browser_action": {
    "browser_style": true,
    "default_popup": "index.html",
    "default_icon": "arrow.png"
  },
  // add an annoying sound
  "web_accessible_resources": [
    "sounds/laugh.mp3"
  ]
}
